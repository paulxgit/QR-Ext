{
	"manifest_version": 2,

	"name": "QR-Generator",
	"description": "Extension to convert the URL to QR-Code.",
	"version": "0.2",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "QR dekho!"
	},

	"icons":{
		"128":"icon.png"
	},

	"permissions": [
		"tabs",
		"activeTab"
	]
}